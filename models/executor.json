{
  "name": "Executor",
  "base": "PersistedModel",
  "properties": {
    "address": {
      "type": "string",
      "required": true
    },
    "APIPort": {
      "type": "string"
    },
    "totalCapacity": {
      "type": "number"
    },
    "remainingCapacity": {
      "type": "number",
      "default": -1
    }
  },
  "relations": {
    "instances": {
      "type": "hasMany",
      "model": "ServiceInstance"
    },
    "services": {
      "type": "hasMany",
      "through": "ServiceInstance",
      "model": "ServerService"
    }
  },
  "acls": []
}

